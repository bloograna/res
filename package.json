  "author": "Nadia",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/bloograna/res"
  },
  "scripts": {
    "clean": "npx rimraf coverage dist node_modules target",
    "start": "npx webpack-dev-server --colors --profile --progress --watch",
    "build": "npx webpack",
    "test": "npm run prettier:check && npm run lint && npm run test:unit",
    "test:unit": "npx jest",
    "fix": "npm run prettier:fix && npm run lint:fix",
    "lint": "npx eslint \"$npm_package_paths_lintable\"",
    "lint:fix": "npx eslint \"$npm_package_paths_lintable\" --fix",
    "precommit": "npx lint-staged && npm run test:unit && npm run prettier:fix",
    "prettier:check": "npx prettier --no-config --no-editorconfig --single-quote --list-different \"$npm_package_paths_lintable\"",
    "prettier:debug": "npx prettier --no-config --no-editorconfig --single-quote --debug-check \"$npm_package_paths_lintable\"",
    "prettier:fix": "npx prettier --no-config --no-editorconfig --single-quote --write \"$npm_package_paths_lintable\"",
    "postversion": "git commit -am \"[package.json] prepare release ${releaseVersion}\" && git push --set-upstream origin ${branch}"
  },
  "lint-staged": {
    "{*.js,app/**/*.js}": [
      "eslint",
      "git add"
    ]
  },
  "paths": {
    "lintable": "{*.js,src/**/*.js}"
  },
